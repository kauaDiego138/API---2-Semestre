      "source": [
        "caminho_saida = '/content/drive/My Drive/API-2/RENAEST/Tratados/'\n",
        "\n",
        "# cria a pasta se não existir\n",
        "import os\n",
        "os.makedirs(caminho_saida, exist_ok=True)\n",
        "\n",
        "# salva os arquivos tratados\n",
