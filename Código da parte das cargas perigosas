    {
      "cell_type": "code",
      "source": [
        "cargas_perigosas['Ano'] = pd.to_numeric(cargas_perigosas['Ano'], errors='coerce')\n",
        "\n",
        "cargas_perigosas = cargas_perigosas.dropna(subset=['Ano'])\n",
        "\n",
        "cargas_perigosas['Ano'] = cargas_perigosas['Ano'].astype(int)\n",
        "\n",
        "cargas_perigosas = cargas_perigosas.sort_values(by='Ano', ascending=True)\n",
        "\n",
        "cargas_perigosas = cargas_perigosas.reset_index(drop=True)"
      ],
